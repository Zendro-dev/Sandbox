{
  "model": "person",
  "storageType": "zendro-webservice-adapter",
  "adapterName": "person_public",
  "regex": "zendro_public",
  "url": "http://graphql.zendro-dev.org/graphql",
  "attributes": {
		"id": "String",
		"name": "String",
		"email": "String",
		"role": "String",
		"affiliation": "String",

		"investigation_ids": "[String]",
		"study_ids": "[String]"
  },
  "associations": {
		"investigations": {
			"type": "many_to_many",
			"reverseAssociation": "people",
			"implementation": "foreignkeys",
      "target": "investigation",
			"targetStorageType": "distributed-data-model",
      "targetKey": "person_ids",
      "sourceKey": "investigation_ids",
      "keysIn": "person"
		},
		"studies": {
			"type": "many_to_many",
			"reverseAssociation": "people",
			"implementation": "foreignkeys",
      "target": "study",
			"targetStorageType": "distributed-data-model",
      "targetKey": "person_ids",
      "sourceKey": "study_ids",
      "keysIn": "person"
		}
  },
  "internalId": "id"
}