{
  "model": "observed_variable",
  "storageType": "presto",
  "attributes": {
		"observation_variable_id": "Int",
    "trait": "String",
		"method": "String",
		"scale": "String",
		"method_description": "String",
		"time_scale": "DateTime",
		"external_data_file": "String",
		"study_id": "Int",
		"observation_unit_id": "String"
  },
	"associations": {
		"observation_unit": {
			"type": "many_to_one",
			"reverseAssociation": "observed_variables",
      "implementation": "foreignkeys",
      "targetStorageType": "presto",
      "target": "observation_unit",
      "targetKey": "observation_unit_id",
      "keysIn": "observed_variable"
		},
		"study": {
			"type": "many_to_one",
			"implementation": "foreignkeys",
			"target": "study",
			"targetKey": "study_id",
			"keysIn": "observed_variable",
			"targetStorageType": "presto",
			"reverseAssociation":"observed_variables"
		}
	},
	"internalId": "observation_variable_id",
	"useDataLoader": true
}
